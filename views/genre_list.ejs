<!DOCTYPE html>
<html>
  <head>
    <%- include('./partials/head'); %>
  </head>
  <body>
    <div class="wrapper">
      <%- include('./partials/header'); %>
      <main>
        <%- include('./partials/nav'); %>
        <div class="content">
          <h1>Viewing genres</h1>
          <div class="list-container">
            <% genre_list.forEach((genre) => { %>
            <a href="<%= genre.url %>"><%= genre.name %> </a>
            <%})%>
          </div>
        </div>
      </main>
      <%- include('./partials/footer'); %>
    </div>
  </body>
</html>
